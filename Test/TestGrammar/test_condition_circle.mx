int run()
{
    for(int i = 1; i < 5; ++i)
    {
        circle = circle + 1;
        energy = energy - 1;
    }
    while(energy != 0)
    {
        if(circle % 5 == 0)
        {
            continue;
        }
        else
        {
            speed++;
        }
        circle++;
        energy--;
        if(circle >= 50)
            break;
    }
    return circle;
}